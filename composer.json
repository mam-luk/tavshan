{
  "name": "mamluk/tavshan",
  "type": "library",
  "description": "A simplified Rabbit MQ library written on top of the PHP AMQP Library",
  "keywords": [
    "Rabbit MQ",
    "Queues"
  ],
  "homepage": "https://mamluk.net",
  "license": "MIT",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=8.2",
    "php-amqplib/php-amqplib": "^3.7",
    "monolog/monolog": "^3.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Mamluk\\Tavshan\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mamluk\\Tavshan\\Tests\\": "tests/"
    }
  }
}
